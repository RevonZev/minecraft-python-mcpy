{
    "execute": {
        "variations": [
            {
                "pattern": "^as\\sat\\s.+:$",
                "command": "as {value} at @s",
                "replace": ["as at ", ":"],
                "execute": true
            },
            {
                "pattern": "^as\\s.+:$",
                "command": "as {value}",
                "replace": ["as ", ":"],
                "execute": true
            }
        ]
    }
}