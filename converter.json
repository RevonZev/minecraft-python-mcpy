{
    "execute": [
        {
            "pattern": "^as\\sat\\s.+:$",
            "command": "as {value} at @s",
            "replace": ["as at ", ":"],
            "execute": true
        },
        {
            "pattern": "as\\s[^(?!.*at)].+:",
            "command": "as {value}",
            "replace": ["as ", ":"],
            "execute": true
        }
    ]
}